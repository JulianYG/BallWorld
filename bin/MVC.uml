            <eAnnotations xmi:id="_tk4ypWNbEeWnVf-kUifj5A" source="http://www.eclipse.org/uml2/2.0.0/UML">
              <details xmi:id="_tk4ypmNbEeWnVf-kUifj5A" key="final"/>
            </eAnnotations>
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <ownedParameter xmi:id="_tk4yp2NbEeWnVf-kUifj5A" type="_tj0boWNbEeWnVf-kUifj5A" direction="return"/>
          <language>java</language>
          <body>if (null == stratName)
			System.out.println(&quot;Error: please enter name of the shape!&quot;);
		return new IPaintStrategyFac() {
			public IPaintStrategy makePaintStrategy() {
				return loadPaintStrategy(fixPaintStrategyName(stratName));
			}

			public String toString() {
				return stratName;
			}
		};
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tk4yqGNbEeWnVf-kUifj5A" name="makeUpdateStrategyFac" specification="_thkY4mNbEeWnVf-kUifj5A">
          <ownedParameter xmi:id="_tk4yqWNbEeWnVf-kUifj5A" name="classname">
            <eAnnotations xmi:id="_tk4yqmNbEeWnVf-kUifj5A" source="http://www.eclipse.org/uml2/2.0.0/UML">
              <details xmi:id="_tk4yq2NbEeWnVf-kUifj5A" key="final"/>
            </eAnnotations>
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <ownedParameter xmi:id="_tk4yrGNbEeWnVf-kUifj5A" type="_tj1CsGNbEeWnVf-kUifj5A" direction="return"/>
          <language>java</language>
          <body>if (null == classname)
			return errorStrategyFac;
		return new IUpdateStrategyFac() {
			/**
			 * Instantiate a strategy corresponding to the given class name.
			 * 
			 * @return An IUpdateStrategy instance
			 */
			public IUpdateStrategy makeUpdateStrategy() {
				// In the case of a switcher, we're going to use the same one
				// for everyone
				if (classname.equals(&quot;Switcher&quot;)) {
					return new StraightStrategy();
				}
				return loadUpdateStrategy(fixUpdateStrategyName(classname));
