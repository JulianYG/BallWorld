        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tk7O42NbEeWnVf-kUifj5A" name="bounce" specification="_th6-MGNbEeWnVf-kUifj5A">
          <language>java</language>
          <body>// If the wall is reached, bounce the ball back by changing its direction.
		if (getxPos() > pnlCanvas.getWidth() - getRadius()) { //2 *
			setxVel(-1 * getxVel());
			setxPos(pnlCanvas.getWidth() - getRadius()); //2*
		} else if (getxPos() - getRadius() &lt;= 0) { // none
			setxVel(-1 * getxVel());
			setxPos(getRadius()); // 0
		} else if (getyPos() >= pnlCanvas.getHeight() - getRadius()) { //2*
			setyVel(-1 * getyVel());
			setyPos(pnlCanvas.getHeight() - getRadius()); //2*
		} else if (getyPos() - getRadius() &lt;= 0) { // none
			setyVel(-1 * getyVel());
			setyPos(getRadius()); // 0
		}
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tk7O5GNbEeWnVf-kUifj5A" name="move" specification="_th6-MWNbEeWnVf-kUifj5A">
          <language>java</language>
          <body>// New Position = Old Position + Velocity.
		setxPos(getxPos() + getxVel());
		setyPos(getyPos() + getyVel());
	</body>
        </ownedBehavior>
