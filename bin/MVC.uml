        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tk4LlWNbEeWnVf-kUifj5A" name="loadPaintStrategy" specification="_thijsWNbEeWnVf-kUifj5A">
          <ownedParameter xmi:id="_tk4LlmNbEeWnVf-kUifj5A" name="paintStrat">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <ownedParameter xmi:id="_tk4Ll2NbEeWnVf-kUifj5A" type="_tj4GAWNbEeWnVf-kUifj5A" direction="return"/>
          <language>java</language>
          <body>try {
			Object[] args = {};
			java.lang.reflect.Constructor&lt;?> cs[] = Class.forName(paintStrat).getConstructors();
			java.lang.reflect.Constructor&lt;?> c = null;
			for (int i = 0; i &lt; cs.length; i++) {
				// find the first constructor with the right number of input parameters 
				if (args.length == (cs[i]).getParameterTypes().length) {
					c = cs[i];
					break;
				}
			}
			return (IPaintStrategy) c.newInstance(args);
			// Call the constructor. Will throw a null ptr exception if no constructor with the right number of input parameters was found.
		} catch (Exception ex) {
			System.err.println(&quot;Class &quot; + paintStrat + &quot; failed to load. \nException = \n&quot; + ex);
			ex.printStackTrace(); // print the stack trace to help in debugging.
			return null; // Is this really a useful thing to return here? Is
							// there something better that could be returned?
		}
	</body>
        </ownedBehavior>
