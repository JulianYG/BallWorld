        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tk6AwmNbEeWnVf-kUifj5A" name="BallGUI" specification="_tkE6UGNbEeWnVf-kUifj5A">
          <ownedParameter xmi:id="_tk6Aw2NbEeWnVf-kUifj5A" name="modelCtrlAdpt" type="_th0QgGNbEeWnVf-kUifj5A"/>
          <ownedParameter xmi:id="_tk6AxGNbEeWnVf-kUifj5A" name="modelUpdateAdpt" type="_thgugGNbEeWnVf-kUifj5A"/>
