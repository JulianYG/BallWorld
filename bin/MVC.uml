        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tk3kh2NbEeWnVf-kUifj5A" name="update" specification="_tkRuoGNbEeWnVf-kUifj5A">
          <ownedParameter xmi:id="_tk3kiGNbEeWnVf-kUifj5A" name="g" type="_tjzNgWNbEeWnVf-kUifj5A"/>
          <language>java</language>
          <body>// Update all balls.
		myDispatcher.notifyAll((IBallCmd) (context, disp) -> {
			context.getUpdateStrategy().updateState(context);
			context.move();
			context.bounce();
			context.getPaintStrategy().paint(context, g);
		});
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tk4LkGNbEeWnVf-kUifj5A" name="start" specification="_thh8oGNbEeWnVf-kUifj5A">
          <language>java</language>
          <body>timer.start();
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tk4LkWNbEeWnVf-kUifj5A" name="loadBall" specification="_tkSVsGNbEeWnVf-kUifj5A">
          <ownedParameter xmi:id="_tk4LkmNbEeWnVf-kUifj5A" name="updateStrategy" type="_tj4GA2NbEeWnVf-kUifj5A"/>
          <ownedParameter xmi:id="_tk4Lk2NbEeWnVf-kUifj5A" name="paintStrat" type="_tj4GAWNbEeWnVf-kUifj5A"/>
          <language>java</language>
          <body>Ball b = makeBall(model2ViewAdpt.getPnlCanvas());
		b.setUpdateStrategy(updateStrategy);
		b.setPaintStrategy(paintStrat);

		if (null == paintStrat) {
			System.out.println(&quot;Null IPaintStrategy in BallModel.loadBall&quot;);
		}

		// Add the ball to dispatcher.
		myDispatcher.addObserver(b);
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tk4LlGNbEeWnVf-kUifj5A" name="clearAllBalls" specification="_thijsGNbEeWnVf-kUifj5A">
          <language>java</language>
          <body>// Delete all balls from dispatcher.
		myDispatcher.deleteObservers();
	</body>
